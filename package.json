{
  "name": "@minch/nuxt-feedback",
  "version": "0.6.8",
  "description": "Easily integrate a feedback widget into your Nuxt app.",
  "repository": "oneminch/Nuxt-Feedback",
  "homepage": "https://github.com/oneminch/Nuxt-Feedback/#readme",
  "license": "MIT",
  "author": "Dawit (https://github.com/oneminch/)",
  "type": "module",
  "keywords": [
    "nuxt",
    "feedback",
    "nuxt-module",
    "feedback-widget"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    },
    "./runtime/*": "./dist/runtime/*"
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build:css": "pnpm dlx @tailwindcss/cli -i src/runtime/assets/css/tailwind.css -o src/runtime/assets/css/base.css --minify",
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:preview": "nuxt preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "release": "pnpm run prepack && pnpm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^4.0.3",
    "@tailwindcss/vite": "^4.1.12",
    "@vueuse/core": "^13.8.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "defu": "^6.1.4",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-vue-next": "^0.539.0",
    "reka-ui": "^2.5.0",
    "resend": "^6.0.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.12",
    "tw-animate-css": "^1.3.7",
    "vaul-vue": "^0.4.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint": "1.9.0",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.0.3",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.0.21"
  },
  "packageManager": "pnpm@10.15.0"
}
