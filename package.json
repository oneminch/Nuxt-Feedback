{
  "name": "nuxt-feedback-widget",
  "version": "0.5.0",
  "description": "Easily integrate a feedback widget into your Nuxt app.",
  "repository": "oneminch/nuxt-feedback-widget",
  "license": "MIT",
  "author": "Dawit (https://github.com/oneminch/)",
  "type": "module",
  "keywords": [
    "nuxt",
    "feedback",
    "nuxt-module",
    "feedback-widget"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxt dev playground",
    "dev:build": "nuxt build playground",
    "dev:preview": "nuxt preview playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxt prepare playground",
    "default-release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "release": "npm run prepack && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.7",
    "@tailwindcss/vite": "^4.1.11",
    "@vueuse/core": "^13.5.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "defu": "^6.1.4",
    "isomorphic-dompurify": "^2.26.0",
    "lucide-vue-next": "^0.511.0",
    "reka-ui": "^2.3.2",
    "resend": "^4.7.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tw-animate-css": "^1.3.5",
    "vaul-vue": "^0.4.1"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint": "1.3.0",
    "@nuxt/eslint-config": "^1.5.2",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.7",
    "@nuxt/test-utils": "^3.19.2",
    "@types/node": "latest",
    "changelogen": "^0.6.2",
    "eslint": "^9.31.0",
    "nuxt": "^3.17.7",
    "typescript": "~5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.11.0"
}
